{
  "name": "{{ name }}",
  {{#if_eq builder 'packager'}}
    "productName": "{{ name }}",
  {{/if_eq}}
  "version": "{{ appver }}",
  "author": "{{ author }}",
  "description": "{{ description }}",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    {{#if_eq builder 'packager'}}
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    {{else}}
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    {{/if_eq}}
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    {{#if e2e}}
    "e2e": "npm run pack && mocha test/e2e",
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src{{#testing unit e2e}} test{{/testing}}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src{{#testing unit e2e}} test{{/testing}}",
    {{/if}}
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    "unit": "karma start test/unit/karma.conf.js",
    {{/if}}
    "postinstall": "{{#if eslint}}npm run lint:fix{{/if}}"
  },
  {{#if_eq builder 'builder'}}
  "build": {
    "productName": "{{ name }}",
    "appId": "{{ appid }}",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  {{/if_eq}}
  "dependencies": {
    "vue": "^2.6.12"{{deps plugins}}{{depsf framework}}
  },
  "devDependencies": {
    "ajv": "^7.0.2",
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    {{#if_eq framework 'quasar'}}
    "@quasar/extras": "^1.9.12",
    {{/if_eq}}
    "babel-loader": "^8.2.2",
    "babel-register": "^6.26.0",
    "cfonts": "^2.9.1",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "del": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^11.1.1",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    {{#if_eq builder 'packager'}}
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.4",
    {{else}}
    "electron-builder": "^22.9.1",
    {{/if_eq}}
    {{#eslint}}
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.1",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.4.0",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    {{/if_eq}}
    {{/eslint}}
    "mini-css-extract-plugin": "^1.3.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    {{#if unit}}
    "inject-loader": "^4.0.1",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.3",
    "karma-electron": "^6.3.1",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    {{/if}}
    {{#if e2e}}
    "require-dir": "^1.2.0",
    "spectron": "^13.0.0",
    {{/if}}
    {{#testing unit e2e}}
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    {{/testing}}
    "multispinner": "^0.2.1",
    "node-loader": "^1.0.2",
    {{#if_eq csspreprocessor 'sass'}}
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    {{/if_eq}}
    {{#if_eq csspreprocessor 'less'}}
    "less": "^4.0.0",
    "less-loader": "^7.2.0",
    {{/if_eq}}
    {{#if_eq csspreprocessor 'stylus'}}
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.1",
    {{/if_eq}}
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3"
  }
}
